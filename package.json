{
  "name": "mock-yaml-server",
  "version": "1.0.5",
  "author": "Abhijit Darji",
  "description": "YAML based mock REST API server",
  "main": "src/server.js",
  "bin": "src/bin.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/abhijitdarji/mock-yaml-server.git"
  },
  "homepage": "https://github.com/abhijitdarji/mock-yaml-server",
  "type": "module",
  "exports": {
    ".": "./src/server.js"
  },
  "scripts": {
    "start": "node src/server.js -f ./resources",
    "watch": "node src/watcher.js -f ./resources",
    "publish:local": "npm publish --registry http://localnpm:4873",
    "unpublish:local": "npm unpublish --force --registry http://localnpm:4873"
  },
  "keywords": [
    "mock",
    "yaml",
    "server",
    "fake",
    "REST",
    "api",
    "test",
    "testing",
    "swagger",
    "openapi",
    "data",
    "dummy",
    "faker-js"
  ],
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "9.9.0",
    "chalk": "5.4.1",
    "chokidar": "4.0.3",
    "cors": "2.8.5",
    "express": "5.1.0",
    "genson-js": "0.0.8",
    "joi": "17.13.3",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "lowdb": "7.0.1",
    "pluralize": "8.0.0",
    "swagger-ui-express": "5.0.1",
    "uuid": "11.1.0",
    "yargs": "18.0.0"
  }
}
